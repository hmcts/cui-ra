    "mini-css-extract-plugin": "^2.9.0",
    "mocha": "^10.0.0",
    "mocha-multi": "^1.1.6",
    "mochawesome": "^7.1.2",
    "nock": "^13.5.4",
    "nodemon": "^3.1.0",
    "pa11y": "^8.0.0",
    "playwright": "^1.43.1",
    "prettier": "^3.2.5",
    "puppeteer": "^22.7.1",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "sinon": "^17.0.1",
    "striptags": "3.2.0",
    "style-loader": "^4.0.0",
    "supertest": "^7.0.0",
    "traverse": "^0.6.9",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "webdriverio": "^6.12.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.2.1",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md,html,json}": "prettier --write"
  },
  "packageManager": "yarn@3.8.1",
  "resolutions": {
    "axios": "^1.6.7"
  }
}
