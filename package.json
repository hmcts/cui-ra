{
  "name": "cui-ra",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "start:test": "NODE_ENV=test nodemon --config nodemon.json",
    "lint": "stylelint **/*.scss -v -q && eslint **/*.ts **/*.js --no-warn-ignored && prettier --check .",
    "lint:fix": "prettier --write . .github && eslint --fix .",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "fortifyScan": "./src/test/java/gradlew -p src/test/java fortifyScan",
    "test": "npx if-env CI=true && exit 0 || yarn test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest --detectOpenHandles --forceExit -c jest.routes.config.js",
    "test:a11y": "jest --detectOpenHandles --forceExit -c jest.a11y.config.js --maxWorkers 1",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=1 jest -c jest.smoke.config.js",
    "test:fullfunctional": "yarn playwright install && NODE_TLS_REJECT_UNAUTHORIZED=1 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps",
    "test:functional": "NODE_TLS_REJECT_UNAUTHORIZED=1 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps",
    "test:functional:new-journey-e2e": "NODE_TLS_REJECT_UNAUTHORIZED=1 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps --grep @new-journey-e2e",
    "test:functional:existing-journey-e2e": "NODE_TLS_REJECT_UNAUTHORIZED=1 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps --grep @existing-journey-e2e",
    "test:functional:new-journey-errors": "NODE_TLS_REJECT_UNAUTHORIZED=1 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps --grep @new-journey-errors",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh",
    "test-crossbrowser-e2e": "NODE_ENV=testing ALLOW_CONFIG_MUTATIONS=true LOG_LEVEL=OFF NODE_TLS_REJECT_UNAUTHORIZED=1 codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} -c src/test/crossbrowser/saucelabs.conf.js",
    "cichecks": "yarn add puppeteer && yarn build && yarn rebuild puppeteer && yarn lint && yarn test && yarn test:routes",
    "prepare": "husky install",
    "pvmount:demo": "REMOTE_ENV=cui-demo NAMESPACE=cui node bin/mount-property-volume.js",
    "pvmount:aat": "REMOTE_ENV=cui-aat NAMESPACE=cui node bin/mount-property-volume.js",
    "pvmount:perftest": "REMOTE_ENV=cui-perftest NAMESPACE=cui node bin/mount-property-volume.js",
    "pvmount:ithc": "REMOTE_ENV=cui-ithc NAMESPACE=cui node bin/mount-property-volume.js"
  },
  "dependencies": {
    "@hmcts/cookie-manager": "^1.0.0",
    "@hmcts/info-provider": "^1.2.1",
    "@hmcts/nodejs-healthcheck": "^1.8.5",
    "@hmcts/nodejs-logging": "^4.0.4",
    "@hmcts/properties-volume": "^1.1.0",
    "@types/body-parser": "^1.19.5",
    "@types/config": "^3.3.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/csurf": "^1.11.5",
    "@types/es6-promisify": "^6.0.4",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/i18n": "^0.13.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "20.11.20",
    "@types/nunjucks": "^3.2.6",
    "@types/require-directory": "^2.1.6",
    "@types/serve-favicon": "^2.5.7",
    "accessible-autocomplete": "^3.0.0",
    "ajv": "^8.12.0",
