{
  "name": "cui-ra",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "start:test": "NODE_ENV=test nodemon --config nodemon.json",
    "lint": "stylelint **/*.scss -v -q && eslint . --ext .js,.ts && prettier --check .",
    "lint:fix": "prettier --write . .github && eslint --fix .",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "test": "npx if-env CI=true && exit 0 || yarn test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest --detectOpenHandles --forceExit -c jest.routes.config.js",
    "test:a11y": "jest --detectOpenHandles --forceExit -c jest.a11y.config.js --maxWorkers 1",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=1 jest -c jest.smoke.config.js",
    "test:functional": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps",
    "test:fullfunctional": "NODE_TLS_REJECT_UNAUTHORIZED=1 codeceptjs run --config ./src/test/functional/codecept.conf.js --steps --grep @allJourney",
    "cichecks": "yarn add puppeteer && yarn build && yarn rebuild puppeteer && yarn lint && yarn test && yarn test:routes && yarn test:a11y",
    "prepare": "husky install",
    "pvmount:demo": "REMOTE_ENV=cui-demo NAMESPACE=cui node bin/mount-property-volume.js",
    "pvmount:aat": "REMOTE_ENV=cui-aat NAMESPACE=cui node bin/mount-property-volume.js",
    "pvmount:perftest": "REMOTE_ENV=cui-perftest NAMESPACE=cui node bin/mount-property-volume.js",
    "pvmount:ithc": "REMOTE_ENV=cui-ithc NAMESPACE=cui node bin/mount-property-volume.js"
  },
  "dependencies": {
    "@hmcts/info-provider": "^1.0.0",
    "@hmcts/nodejs-healthcheck": "^1.7.3",
    "@hmcts/nodejs-logging": "^4.0.4",
    "@hmcts/properties-volume": "^1.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/es6-promisify": "^6.0.1",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/helmet": "^4.0.0",
    "@types/i18n": "^0.13.6",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "@types/nunjucks": "^3.2.2",
    "@types/require-directory": "^2.1.2",
    "@types/serve-favicon": "^2.5.3",
    "accessible-autocomplete": "^2.0.4",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "applicationinsights": "^2.3.4",
    "autobind-decorator": "^2.4.0",
    "awilix": "^8.0.1",
    "axios": "^1.0.0",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express": "^4.18.1",
