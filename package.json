    "otplib": "^12.0.1",
    "puppeteer": "^21.3.5",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "require-directory": "^2.1.1",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
