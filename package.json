    "ajv-formats": "^3.0.1",
    "applicationinsights": "^2.9.4",
    "autobind-decorator": "^2.4.0",
    "awilix": "^10.0.2",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.11",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dev": "^0.1.3",
    "express": "^4.19.2",
    "express-nunjucks": "^3.1.2",
