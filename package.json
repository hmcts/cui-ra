    "@types/session-file-store": "^1.2.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "axios-debug-log": "^1.0.0",
    "babel-loader": "^9.1.3",
    "chai": "^4.4.1",
    "chokidar": "^3.6.0",
    "codeceptjs": "^3.5.14",
